    "@prisma/client": "^6.0.0",
    "@prisma/extension-accelerate": "^3.0.0",
    "@types/node": "^22.15.3",
    "dotenv": "^17.2.3",
    "prisma": "^6.0.0",
    "ts-node": "^10.9.2"
  }
}
